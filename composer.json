{
    "name": "jarves\/jarves-bundle",
    "description": "An open-source Content-Management-System CMS as Symfony Bundle.",
    "keywords": [
        "CMS",
        "PHP CMS",
        "WCMS"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:marcj/jarvespublicationbundle.git"
        },
        {
            "type": "vcs",
            "url":  "git@bitbucket.org:marcj/jarvesdemotheme.git"
        }
    ],
    "license": "LGPL-3.0",
    "homepage": "http:\/\/jarves.io\/",
    "authors": [
        {
            "name": "Marc J. Schmidt",
            "homepage": "https:\/\/github.com\/marcj",
            "email": "marc@jarves.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jarves\\": ""
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "symfony/symfony": ">=2.4",
        "twig/extensions": ">=1.0",
        "symfony/monolog-bundle": ">=2.4",
        "sensio/distribution-bundle": ">=2.3",
        "sensio/framework-extra-bundle": ">=2.3",
        "incenteev/composer-parameter-handler": ">=2.0",
        "propel/propel-bundle": "2.0.*@dev",
        "sybio/image-workshop": ">=2",
        "michelf/php-markdown": ">=1.3",
        "composer\/composer": "1.0.*@dev",
        "jarves\/demotheme-bundle": "dev-master",
        "jarves\/publication-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "1.1.*",
        "jms/serializer-bundle": "0.12.*",
        "willdurand/hateoas": "2.0.*@dev",
        "nelmio/api-doc-bundle": "~2.5",
        "richthegeek/phpsass": "dev-master",
        "icap/html-diff": ">=1.0.1"
    },
    "require-dev": {
        "phpunit\/phpunit": "~3.7"
    }
}
